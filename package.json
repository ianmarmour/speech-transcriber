{
  "name": "whisper-js",
  "version": "0.0.1",
  "description": "A simple portable library for working with OpenAIs Whisper model.",
  "main": "dist/whisper.js",
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 model/*.onnx dist/model/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianmarmour/whisper-js.git"
  },
  "keywords": [
    "OpenAI",
    "Whisper",
    "Transcript",
    "Transcriber",
    "Audio",
    "Speech",
    "Speech-to-Text"
  ],
  "author": "Ian Armour <ianmarmour@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ianmarmour/whisper-js/issues"
  },
  "homepage": "https://github.com/ianmarmour/whisper-js#readme",
  "devDependencies": {
    "@types/node": "^20.8.9",
    "copyfiles": "^2.4.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "node-llama-cpp": "^2.8.0",
    "onnxruntime-web": "^1.16.1",
    "speech-recorder": "^2.1.0"
  }
}
